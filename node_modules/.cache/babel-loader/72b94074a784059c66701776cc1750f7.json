  this.message = 'Player X wins!'\n        this.isDisabled = true\n        this.counterFirstPlayer ++\n        this.countGame ++\n        this.victoryLine()\n        return true\n      } else if (this.isVictorySecondPlayer) {\n        this.message = 'Player O wins!'\n        this.isDisabled = true\n        this.counterSecondPlayer ++\n        this.countGame ++\n        this.victoryLine() \n        return true\n      } else {\n        return false\n      }    \n    },\n    victoryLine() {\n      let winningPositions = this.isVictoryFirstPlayer ? this.playerX : this.playerO;\n      for (let rule of this.victoryRules) {\n        if (rule.every(position => winningPositions.includes(position))) {\n          this.lineTr = rule.join('');\n        }\n      }\n    },\n    resumeGame() {\n      if (this.countGame % 2 === 0) {\n        this.currentPlayer = 'playerX'\n      } else {\n        this.currentPlayer = 'playerO'\n      }\n      this.playerX = []\n      this.playerO = []\n      this.message = 'Player turn: ' + this.currentPlayer.replace('player', '')\n      this.isDisabled = false\n      this.lineTr = ''\n      this.isVictoryFirstPlayer = false\n      this.isVictorySecondPlayer = false\n      this.count = 0\n\n      this.items.forEach(item => {\n        item.name = ''\n        item.isClicked = false\n      });\n\n      this.hideVictoryLines()\n      this.resetAnimation()\n    },\n    resetAnimation() {\n      this.items.forEach((item) => {\n        item.animation = false\n      })\n      console.log\n    },\n    resetCount () {\n      this.counterFirstPlayer = 0\n      this.counterSecondPlayer = 0\n      this.countGame = 0\n      this.resumeGame() \n    },\n    hideVictoryLines() {\n      for (let i = 1; i <= 9; i++) {\n        this.$refs['line' + i][0].style.display = 'none'\n      }\n    }\n  },\n};\n</script>\n\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n 